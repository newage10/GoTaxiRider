{
  "name": "GoTaxiRider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier --write . && eslint --fix .",
    "android_clean": "cd android && ./gradlew clean && cd ..",
    "build_android_bundle": "cd android && ./gradlew app:bundleRelease && cd ..",
    "build_android_debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew app:assembleDebug && cd ..  && adb install -r ./android/app/build/outputs/apk/debug/app-debug.apk",
    "build_android_apk": "cd android && ./gradlew app:assembleRelease && cd .. && adb install -r ./android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "@babel/plugin-proposal-do-expressions": "^7.22.5",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/geolocation": "^3.1.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@react-navigation/stack": "^6.3.20",
    "crypto-js": "^3.3.0",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-maps": "^1.8.0",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.10.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.7.3",
    "react-native-screens": "^3.27.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-template-typescript": "^6.12.11",
    "react-redux": "^8.1.3",
    "reactotron-react-native": "^5.0.3",
    "reactotron-redux-saga": "^4.2.3",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
